{
  "id": "debian-12-swarm",
  "name": "Debian 12 Swarm Node",
  "boot": {
    "kernel": "/assets/debian-12/linux",
    "initrd": ["/assets/debian-12/initrd.gz"],
    "args": [
      "initrd=initrd.gz",
      "auto=true",
      "priority=critical",
      "url=http://BARBARY_IP:8480/generic?mac=${mac:hexhyp}",
      "hostname={{.hostname}}",
      "domain=in.hypyr.space",
      "console=tty0",
      "console=ttyS0,115200n8"
    ]
  },
  "generic_id": "debian-preseed.cfg"
}
