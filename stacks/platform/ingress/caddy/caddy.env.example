# Caddy reverse proxy configuration
# Create in 1Password as "caddy.env" with tag "stack:caddy"
# Exported to: /mnt/apps01/secrets/caddy/caddy.env

TZ=America/Chicago

# used for ACME + caddy email label
CADDY_EMAIL=you@example.com

# set in Komodo (secret)
CLOUDFLARE_API_TOKEN=

# optional: match TrueNAS SCALE user/group IDs for file ownership
PUID=1701
PGID=1702
