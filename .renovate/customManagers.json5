{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  customManagers: [
    {
      customType: "regex",
      description: "Process annotated dependencies",
      fileMatch: ["/(^|/).+\\.ya?ml(?:\\.j2)?$/"],
      matchStrings: [
        "datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)\\n.+ (?<currentValue>(?:v|\\d)\\S+)",
        "datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)\\n.+/(?<currentValue>(?:v|\\d)[^/]+)"
      ],
      datasourceTemplate: "{{#if datasource}}{{{datasource}}}{{else}}github-releases{{/if}}"
    },
    {
      customType: "regex",
      description: "Process mise.toml talosctl tool versions",
      fileMatch: ["\\.mise\\.toml$"],
      matchStrings: ["talosctl = \"(?<currentValue>[0-9]+\\.[0-9]+\\.[0-9]+[^\"]*)\""],
      datasourceTemplate: "github-releases",
      depNameTemplate: "siderolabs/talos"
    },
    {
      customType: "regex",
      description: "Process mise.toml kubectl tool versions",
      fileMatch: ["\\.mise\\.toml$"],
      matchStrings: ["kubectl = \"(?<currentValue>[0-9]+\\.[0-9]+\\.[0-9]+[^\"]*)\""],
      datasourceTemplate: "github-releases",
      depNameTemplate: "kubernetes/kubernetes"
    },
    {
      customType: "regex",
      description: "Process mise.toml helm tool versions",
      fileMatch: ["\\.mise\\.toml$"],
      matchStrings: ["helm = \"(?<currentValue>[0-9]+\\.[0-9]+\\.[0-9]+[^\"]*)\""],
      datasourceTemplate: "github-releases",
      depNameTemplate: "helm/helm"
    },
    {
      customType: "regex",
      description: "Process mise.toml kustomize tool versions",
      fileMatch: ["\\.mise\\.toml$"],
      matchStrings: ["kustomize = \"(?<currentValue>[0-9]+\\.[0-9]+\\.[0-9]+[^\"]*)\""],
      datasourceTemplate: "github-releases",
      depNameTemplate: "kubernetes-sigs/kustomize"
    },
    {
      customType: "regex",
      description: "Process mise.toml flux2 tool versions",
      fileMatch: ["\\.mise\\.toml$"],
      matchStrings: ["flux2 = \"(?<currentValue>[0-9]+\\.[0-9]+\\.[0-9]+[^\"]*)\""],
      datasourceTemplate: "github-releases",
      depNameTemplate: "fluxcd/flux2"
    },
    {
      customType: "regex",
      description: "Process system-upgrade Kubernetes version",
      fileMatch: ["/kubernetes/clusters/.+/apps/system-upgrade/system-upgrade-controller/plans/cluster-versions\\.ya?ml$/"],
      matchStrings: ["KUBERNETES_VERSION=(?<currentValue>v?[0-9]+\\.[0-9]+\\.[0-9]+[^\\n]*)"],
      datasourceTemplate: "github-releases",
      depNameTemplate: "kubernetes/kubernetes"
    },
    {
      customType: "regex",
      description: "Process system-upgrade Talos version",
      fileMatch: ["/kubernetes/clusters/.+/apps/system-upgrade/system-upgrade-controller/plans/cluster-versions\\.ya?ml$/"],
      matchStrings: ["TALOS_VERSION=(?<currentValue>v?[0-9]+\\.[0-9]+\\.[0-9]+[^\\n]*)"],
      datasourceTemplate: "github-releases",
      depNameTemplate: "siderolabs/talos"
    }
  ]
}
